Program alamat;
uses crt;

var jalan,no,kota,kodepos:string;

begin
write('masukkan nama jalan');
readln(jalan);
write('masukkan nomor');
readln(no);
write('masukkan nama kota');
readln(kota);
write('masukkan kode pos');
readln(kodepos);
writeln('alamat:jalan',jalan,'no',no,' ',' ',kota,kodepos);
readln;
end.


